  "main": "webpack.config.js",
  "scripts": {
    "start": "set NODE_ENV=development&&nodemon --exec babel-node ./server/server.js",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "deploy:heroku": "git subtree push --prefix dist heroku master",
    "clean": "rimraf dist",
    "build:client": "set NODE_ENV=production&&webpack -p --progress --color",
    "build:server": "set NODE_ENV=production&&babel ./server --out-dir ./dist",
    "serve": "set NODE_ENV=production&&node dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
